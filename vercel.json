{
    "version": 2,
    "builds": [

         { "src": "package.json", 
         
            "use": "@vercel/node" 
        
        },
        
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "regions" : ["bom1"],
    "crons": [
        {
            "path": "/api/scheduler",
            "schedule": "0 0 * * *"
        }
    ]

}