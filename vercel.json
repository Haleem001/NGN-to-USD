{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "regions" : ["bom1"],
    "crons": [
        {
            "path": "/api/scheduler",
            "schedule": "0 0 * * *"
        }
    ]

}